  +link('full_screen.html')| Full
  +link('split_screen.html')| Split
  +link('full_grid.html')| Grid
  +link('classic_page.html')| Page
  +link('classic_list.html')| List
  +link('closed.html')| Closed
  +_a(homeUrl+"404.html")| 404
  +_a(homeUrl+"500.html")| 500