extends ../_templates/html

block append vars
  include _demo-vars
  -
    template    = 'blank-canvas'
    title       = `${template} | ${name}`
    description = `${template} helps create a website from scratch.`
    url         = url+`/${template}.html`

    og_title       = title
    og_description = description
    og_url         = url
  //- 

block append style
  style.
    ._layout { margin:1em }
  //- 

block main
  main#_main
    article This is a #{template}