extends ../pugs/error

block append vars
  include _vars
  - heading = `Temporary closed`
  +updateTitle( `${heading}` )
  +updateDescription( `Sorry, we are currently unavailable. Please come again later.` )

block main
  +main#_main
    h1#_h1 !{heading}
    p#_p !{description}
    nav#_nav: +backlink._btnlink Back
