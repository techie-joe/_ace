extends _html

block append vars
  - heading = `Blank`
  - about   = `A blank page to start building.`
  - updateTitle( `${heading} | ${name}` )
  - updateDescription( `${about}` )
  - updateUrl( siteUrl+'blank.html' )
  - INCLUDE_NAV = false
  - INCLUDE_BACKSWITCH = true
  - HEADER_TYPE = 'SIMPLE'

//- block main

  +main#_main: article
    +pageData#_pageData
  //- main

//- block main